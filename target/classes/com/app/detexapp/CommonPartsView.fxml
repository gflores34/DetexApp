<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.detexapp.CommonPartsController">
   <children>
      <GridPane prefHeight="763.0" prefWidth="800.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="395.0" minWidth="10.0" prefWidth="43.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="714.0" minWidth="10.0" prefWidth="714.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="525.0" minWidth="10.0" prefWidth="43.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="235.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="235.0" minHeight="7.0" prefHeight="36.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="235.0" minHeight="0.0" prefHeight="18.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="707.0" minHeight="10.0" prefHeight="614.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="130.0" minHeight="1.0" prefHeight="61.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TableView fx:id="tableView" prefHeight="600.0" prefWidth="750.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS" />
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#delete" prefHeight="49.0" prefWidth="86.0" style="-fx-background-color: red; -fx-background-radius: 20;" text="Delete" textFill="WHITE" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" prefHeight="49.0" prefWidth="86.0" style="-fx-background-color: blue; -fx-background-radius: 20;" text="Back" textFill="WHITE" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label alignment="CENTER" prefHeight="37.0" prefWidth="57.0" text="Product:" />
                  <ComboBox fx:id="comboBox" onAction="#comboBoxClick" prefHeight="37.0" prefWidth="394.0" />
                  <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadPartsClick" prefHeight="38.0" prefWidth="88.0" style="-fx-background-radius: 20;" text="Load" />
                  <Button fx:id="addPartButton" mnemonicParsing="false" onAction="#addPartClick" prefHeight="49.0" prefWidth="86.0" style="-fx-background-color: green; -fx-background-radius: 20;" text="Add Part" textFill="WHITE" />
               </children>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets left="40.0" right="20.0" />
               </padding>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
