<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane minHeight="800.0" minWidth="800.0" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.detexapp.EmailController">
   <children>
      <ImageView fx:id="detexLogo" fitHeight="86.0" fitWidth="168.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <TextArea fx:id="callInfo" layoutX="161.0" layoutY="78.0" prefHeight="601.0" prefWidth="479.0" />
      <HBox layoutX="514.0" layoutY="711.0" prefHeight="63.0" prefWidth="252.0" spacing="10.0">
         <children>
            <Button fx:id="emailButton" mnemonicParsing="false" onAction="#email" prefHeight="49.0" prefWidth="77.0" style="-fx-background-radius: 20; -fx-background-color: Green; -fx-border-color: black; -fx-border-radius: 20;" text="Email" textFill="WHITE" />
            <Button fx:id="resetButton" mnemonicParsing="false" onAction="#reset" prefHeight="49.0" prefWidth="77.0" style="-fx-background-radius: 20; -fx-background-color: Red; -fx-border-color: black; -fx-border-radius: 20;" text="Reset" textFill="WHITE" />
            <Button fx:id="backButton" cancelButton="true" mnemonicParsing="false" onAction="#back" prefHeight="49.0" prefWidth="77.0" style="-fx-background-radius: 20; -fx-background-color: Blue; -fx-border-color: black; -fx-border-radius: 20;" text="Back" textFill="WHITE" />
         </children>
      </HBox>
   </children>
</AnchorPane>
