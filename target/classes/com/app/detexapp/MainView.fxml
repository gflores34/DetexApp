<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" minHeight="800.0" minWidth="800.0" spacing="75.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.detexapp.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <ImageView fx:id="detexLogo" fitHeight="336.0" fitWidth="628.0" pickOnBounds="true" preserveRatio="true" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
             <Button fx:id="emailButton" onAction="#onEmailButtonClick" prefHeight="62.0" prefWidth="111.0" style="-fx-background-radius: 20;" text="Email" />
            <Button fx:id="partsButton" alignment="CENTER" onAction="#onPartsButtonClick" prefHeight="62.0" prefWidth="111.0" style="-fx-background-radius: 20;" text="Common Parts" textAlignment="CENTER" wrapText="true" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
